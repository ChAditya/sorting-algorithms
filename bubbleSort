function bubbleSort(arr)
  {
    var i,j,temp,length=arr.length;
      for(i=0;i<length;i++)
        {
          for(j=0;j<length-i-1;j++)
            {
              if(arr[j]>arr[j+1])
                {
                  temp = arr[j];
                  arr[j] = arr[j+1];
                  arr[j+1] = temp;
                 }
            }
        }
        return arr;
  }
console.log(bubbleSort([43,4,3,2,1,-21,0,23,-123,234,1,-11]));
